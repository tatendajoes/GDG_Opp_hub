{
  "crons": [
    {
      "path": "/api/cron/daily-digest",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/cron/deadline-reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/process-queue",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/auto-expire",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/cleanup-expired",
      "schedule": "0 2 * * 0"
    }
  ]
}

